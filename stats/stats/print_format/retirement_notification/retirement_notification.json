 "css": ".main-section {\n    font-size:13pt;\n    color:black;\n    font-family: 'Arial-MT', Arial, sans-serif;\n}\n\n.main-heading {\n    font-size:14pt;\n    font-weight:bolder;\n    text-align:center;\n    text-decoration:underline;\n    margin-bottom:8pt !important;\n}\n\n.print-format td {\n    padding:3px !important;\n}\n\n.table-heading {\n    background-color:#d3d3d3;\n    width:30%;\n    font-weight:bold;\n}\n\n\n.content {\n    font-weight:bold;\n    padding:12px;\n    text-align:justify;\n    direction:rtl;\n}\n\ntable {\n    width:100%;\n    border:1px solid black;\n    color:black;\n    direction:ltr;\n    text-align:center;\n}\n\ntr {\n    border:1px solid black;\n}\n\ntd {\n    border:1px solid black;\n    text-align:center;\n}\n\n@media print{\n    .main-section {\n        padding:20px;\n    }\n    .content {\n        padding:8px;\n    }\n    .main-heading {\n        margin-bottom:10px;\n    }\n}",
