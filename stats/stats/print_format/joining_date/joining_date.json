 "css": ".main-section {\n    font-size:12pt;\n    color:black;\n}\n\n.english {\n    direction: ltr;\n    text-align: left;\n    align-content: center;\n}\n\n.arabic {\n    direction: rtl;\n    text-align: right;\n}\n\ntable {\n    width:100%;\n    color:black;\n    border:1px solid black;\n}\n\n.print-format td, .print-format th {\n    padding: 4px !important;\n}\n\n/*td {*/\n/*    border:1px solid black;*/\n/*}*/",
