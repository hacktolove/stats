 "css": ".main-section {\n    font-size:13pt;\n    color:black;\n}\n\n.main-heading {\n    text-align:center;\n    text-decoration:underline;\n    font-weight:bolder;\n}\n\ntable {\n    width:100%;\n    border:1px solid black;\n    color:black;\n    direction:ltr;\n    text-align:center;\n}\n\ntr {\n    border:1px solid black;\n}\n\ntd {\n    border:1px solid black;\n}\n\n.print-format td {\n    padding:4px !important;\n}\n\n.table-heading {\n    background-color:#d3d3d3 ;\n}\n\n@media print{\n    .main-section {\n        padding:10px;\n    }\n}",
