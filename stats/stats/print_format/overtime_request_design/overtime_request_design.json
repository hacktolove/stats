 "html": "{%- from \"templates/print_formats/standard_macros.html\" import add_header, render_field, print_value, fieldmeta, get_width, get_align_class with context -%}\n\n{% if print_settings.repeat_header_footer %}\n    <div id=\"header-html\">\n        {% if letter_head and not no_letterhead %}\n            <div class=\"letter-head\">{{ letter_head }}</div>\n        {% else%}\n             <div>\n               <table>\n                <tr></tr>\n               </table>\n               </div>\n        {% endif %}\n   </div>\n{% endif %}\n\n<!DOCTYPE html>\n<html lang=\"ar\">\n    \n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n</head>\n\n<body>\n    <!-- First Page-->\n    <div style=\"font-size:14px; color:black;\">\n        <h4 style=\"text-align: left; margin-bottom:-25px;\"><b>\u0631\u0642\u0645 \u0627\u0644\u0642\u0631\u0627\u0631 : {{doc.name}}</b></h4>\n        \n        <!--table-->\n        <table style=\"width:100%; color:black; font-size:15px;  font-weight:bold; margin:0px;\">\n            <thead>\n                <tr>\n                    <td colspan=\"5\" style=\"font-size:15px;\">\n                        <h3 class=\"text-center\" style=\"margin-bottom:5px;\">\u0642\u0631\u0627\u0631 \u062a\u0643\u0644\u064a\u0641 \u0628\u0627\u0644\u0639\u0645\u0644 \u062e\u0627\u0631\u062c \u0648\u0642\u062a \u0627\u0644\u062f\u0648\u0627\u0645 \u0627\u0644\u0631\u0633\u0645\u064a</h3>\n                        \n                        {% if doc.sub_department != None %}\n                            {% set department = frappe.db.get_value(\"Department\", doc.sub_department, \"department_name\")%}\n                        {% else %}\n                            {% set department = frappe.db.get_value(\"Department\", doc.main_department, \"department_name\")%}\n                        {% endif %}\n                        <p style=\"line-height:1.8;\">\n                            <b>\n                                         \u0625\u0646 \u0645\u062f\u064a\u0631 \u0639\u0627\u0645 \u0627\u0644\u0645\u0648\u0627\u0631\u062f \u0627\u0644\u0628\u0634\u0631\u064a\u0629 <br>\n                             \u0628\u0646\u0627\u0621\u0627 \u0639\u0644\u0649 \u0627\u0644\u0635\u0644\u0627\u062d\u064a\u0640\u0640\u0640\u0627\u062a \u0627\u0644\u0645\u0645\u0646\u0648\u062d\u0640\u0640\u0629 \u0644\u0640\u0640\u0640\u0647 <br>\n                            </b>\n                        </p>\n                        \n                         <p style=\"line-height:1.8; direction:rtl;\">\n                            <b>\n                                 \u0648\u0628\u0646\u0627\u0621\u064b \u0639\u0644\u0649 \u0645\u0648\u0627\u0641\u0642\u0629 \u0635\u0627\u062d\u0628 \u0627\u0644\u0635\u0644\u0627\u062d\u064a\u0629 \u0648\u062a\u062d\u0642\u064a\u0642\u064b\u0627\u064b \u0644\u0645\u0642\u062a\u0636\u064a\u0627\u062a \u0645\u0635\u0644\u062d\u0629 \u0627\u0644\u0639\u0645\u0644 \u0641\u064a  {{  department }}<br>\n                            </b>\n                        </p>\n                        <p style=\"line-height:1.8; text-align:center;\">\n                            <b>\n                                \u064a\u0642\u0631\u0631 \u0645\u0627 \u064a\u0644\u064a :-\n                            </b>\n                        </p>\n                        <p style=\"line-height:1.8;\">\n                            <b>\n                                \u0623\u0648\u0644\u0627\u064b  : \u062a\u0643\u0644\u064a\u0641 \u0627\u0644\u0645\u0648\u0638\u0641\u064a\u0646 \u0627\u0644\u0645\u0648\u0636\u062d\u0629 \u0623\u0633\u0645\u0627\u0624\u0647\u0645 \u0648\u0628\u064a\u0627\u0646\u0627\u062a\u0647\u0645 \u0648\u0639\u062f\u062f\u0647\u0645 ( {{doc.employee_overtime_request|length}} ) \u0648\u0641\u0642\u0627 \u0644\u0644\u0628\u064a\u0627\u0646 \n                                \u0627\u062f\u0646\u0627\u0647 \u0628\u0627\u0644\u0639\u0645\u0644 \u062e\u0627\u0631\u062c \u0648\u0642\u062a \u0627\u0644\u062f\u0648\u0627\u0645 \u0627\u0644\u0631\u0633\u0645\u064a \u0644\u0645\u062f\u0629 ( {{ doc.total_no_of_days }} ) \u0627\u064a\u0627\u0645 \u0648\u0628\u0645\u0639\u062f\u0644 ( {{ doc.employee_overtime_request[0].no_of_hours_per_day }} )\n                                \u0633\u0627\u0639\u0627\u062a \u064a\u0648\u0645\u064a\u0627 {{ _(doc.day_type) }}\u060c \u0627\u0639\u062a\u0628\u0627\u0631\u0627\u064b \u0645\u0646  {{frappe.utils.getdate(doc.overtime_start_date).strftime(\"%d-%m-%Y\")}}  \u0644\u0646\u0647\u0627\u064a\u0629 {{ frappe.utils.getdate(doc.overtime_end_date).strftime(\"%d-%m-%Y\")}}\n                            </b>\n                        </p>\n                    </td>\n                </tr>\n            </thead>\n           \n            <tbody>\n                <tr>\n                    <td colspan=\"5\">\n                        <div style=\"line-height:1.8; margin-top:0px;\">\n                            <b>\u0648\u0630\u0644\u0643 \u0644\u0644\u0623\u0633\u0628\u0627\u0628 \u0627\u0644\u062a\u0627\u0644\u064a\u0629 :</b>\n                            <br>\n                            <b>{{ _(doc.request_reason)|replace(\"\\n\", \"<br>\") }}</b>\n                        </div>\n                    </td>\n                </tr>\n                <tr>\n                    <th rowspan=\"2\" style=\"border:1px solid black; text-align:center; vertical-align:middle; color:black; font-weight:bold;\">\u0639\u062f\u062f</th>\n                    <th rowspan=\"2\" style=\"border:1px solid black; text-align:center; vertical-align:middle; color:black; font-weight:bold;\">\u0627\u0644\u0627\u0633\u0645</th>\n                    <th rowspan=\"2\" style=\"border:1px solid black; text-align:center; vertical-align:middle; color:black; font-weight:bold;\">\u0627\u0644\u0645\u0631\u062a\u0628\u0629</th>\n                    <th colspan=\"2\" style=\"border:1px solid black; text-align:center; vertical-align:middle; color:black; font-weight:bold;\">\u0627\u0644\u0623\u064a\u0627\u0645 \u0627\u0644\u0641\u0639\u0644\u064a\u0629</th>\n                </tr>\n                \n                <tr>\n                    <th style=\"border:1px solid black; text-align:center; vertical-align:middle; color:black; font-weight:bold;\">\u0639\u0627\u062f\u064a\u0629</th>\n                    <th style=\"border:1px solid black; text-align:center; vertical-align:middle; color:black; font-weight:bold;\">\u0639\u0637\u0644</th>\n                </tr> \n                \n                {%for row in doc.employee_overtime_request %}\n                    <tr>\n                        <td style=\"border:1px solid black; text-align:center; vertical-align:middle;\">{{ loop.index }}</td>\n                        <td style=\"border:1px solid black; text-align:center; vertical-align:middle;\">{{ row.employee_name }}</td>\n                        <td style=\"border:1px solid black; text-align:center; vertical-align:middle;\">{{ row.employee_contract_name}}</td>\n                        <td style=\"border:1px solid black; text-align:center; vertical-align:middle;\">{%if doc.day_type == \"Week Days\" %} {{ row.total_no_of_days }} {% else %} 0 {%endif%}</td>\n                        <td style=\"border:1px solid black; text-align:center; vertical-align:middle;\">{%if doc.day_type == \"Off-Days\" %} {{ row.total_no_of_days }} {% else %} 0 {%endif%}</td>\n                    </tr>\n                {% endfor %}\n            </tbody>\n        </table>\n        \n        <div style=\"text-align: left; font-weight:bold; line-height:1.8; margin-top:10px;\">\n            <p>\u0645\u062f\u064a\u0631 \u0639\u0627\u0645 \u0627\u0644\u0645\u0648\u0627\u0631\u062f \u0627\u0644\u0628\u0634\u0631\u064a\u0629</p>\n            <p>\u0639\u0645\u0631 \u0628\u0646 \u0645\u0631\u0632\u0648\u0642 \u0627\u0644\u0647\u062c\u0644\u0647</p>\n        </div>\n        \n        <p style=\"line-height:1.8;\"><b>\u062b\u0627\u0646\u064a\u0627\u064b :  \u064a\u0635\u0631\u0641 \u0644\u0647\u0645 \u0645\u0627 \u064a\u0633\u062a\u062d\u0642\u0648\u0646\u0647 \u0645\u0646 \u0627\u0644\u0645\u0643\u0627\u0641\u0623\u0629 \u0648\u0641\u0642\u0627\u064b \u0644\u0644\u0646\u0638\u0627\u0645.</b></p>\n        \n        <p style=\"line-height:1.8;\"><b>\u062b\u0627\u0644\u062b\u0627\u064b :   \u064a\u0628\u0644\u063a \u0647\u0630\u0627 \u0627\u0644\u0642\u0631\u0627\u0631 \u0644\u0645\u0646 \u064a\u0644\u0632\u0645 \u0644\u0625\u0646\u0641\u0627\u0630\u0647.</b></p>\n        \n        <!-- Stamp removed and label attached-->\n        <p style=\"font-weight:bold; text-align:center; line-height:1.8; margin-top:25px; direction:rtl;\">\u0647\u0630\u0647 \u0627\u0644\u0648\u062b\u064a\u0642\u0629 \u062a\u0645 \u0625\u0639\u062a\u0645\u0627\u062f\u0647\u0627 \u0625\u0644\u0643\u062a\u0631\u0648\u0646\u064a\u0627 \u0648\u0644\u0627 \u062a\u062d\u062a\u0627\u062c \u0627\u0644\u064a \u062a\u0648\u0642\u064a\u0639 \u064a\u062f\u0648\u064a</p>\n        \n    </div>\n    \n    <!--Page Break-->\n    <!--<div class=\"page-break\"></div>-->\n    \n    <!--Second Page-->\n    \n</body>\n\n</html>\n\n\n{% if print_settings.repeat_header_footer %}\n<div id=\"footer-html\">\n    {% if footer %}\n    <div class=\"letter-head-footer\"> {{ footer }} </div>\n    {% endif %}\n    <p class=\"text-center small page-number visible-pdf\">\n        {{ _(\"Page {0} of {1}\").format('<span class=\"page\"></span>', '<span class=\"topage\"></span>') }} </p>\n</div>\n{% endif %}\n",
