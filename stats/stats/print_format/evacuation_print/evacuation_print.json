 "html": "{%- from \"templates/print_formats/standard_macros.html\" import add_header, render_field, print_value, fieldmeta, get_width, get_align_class with context -%}\n\n{% if print_settings.repeat_header_footer %}\n    <div id=\"header-html\">\n        {% if letter_head and not no_letterhead %}\n            <div class=\"letter-head\">{{ letter_head }}</div>\n        {% else%}\n             <div>\n               <table>\n                <tr></tr>\n               </table>\n               </div>\n        {% endif %}\n   </div>\n{% endif %}\n\n<div class=\"font-size:13pt; color:black;\">\n    \n    <table style=\"font-size:10pt; color:black; width:100%; border:1px solid black; text-align:center; direction:rtl;\">\n        <thead>\n            <tr>\n                <td style=\"border:1px solid black; vertical-align:middle !important; font-weight:bold;\">\ufead\ufed7\ufee2\u200c\u200c \u200c\u200c\ufe8d\ufedf\ufee4\ufecc\ufe8e\ufee3\ufee0\ufe94 </td>\n                <td style=\"border:1px solid black; vertical-align:middle !important;\">{{ doc.name }}</td>\n                <td style=\"border:1px solid black; vertical-align:middle !important; font-weight:bold;\">\u0627\u0644\u062a\u0627\u0631\u064a\u062e</td>\n                <td style=\"border:1px solid black; vertical-align:middle !important;\">{{ doc.date }}</td>\n                <td style=\"border:1px solid black; vertical-align:middle !important; font-weight:bold;\">\u062a\u0627\u0631\u064a\u062e \u0627\u062e\u0631 \u064a\u0648\u0645 \u0639\u0645\u0644</td>\n                <td style=\"border:1px solid black; vertical-align:middle !important;\">{{ doc.last_working_days }}</td>\n                <td style=\"border:1px solid black; vertical-align:middle !important; font-weight:bold;\">\ufee7\ufeee\ufec9\u200c\u200c \u200c\u200c\ufe8d\ufedf\ufee4\ufecc\ufe8e\ufee3\ufee0\ufe94 </td>\n                <td style=\"border:1px solid black; vertical-align:middle !important;\">\u0637\u0644\u0628 \u0625\u062e\u0644\u0627\u0621 \u0627\u0644\u0637\u0631\u0641</td>\n            </tr>\n            \n            <tr>\n                <td style=\"border:1px solid black; vertical-align:middle !important; font-weight:bold;\" colspan=\"2\">\ufe8d\ufedf\ufee4\ufeee\ufebf\ufeee\ufec9</td>\n                <td style=\"border:1px solid black; vertical-align:middle !important;\" colspan=\"6\">\u0637\u0644\u0628 ({{doc.employee_name}}) \u0645\u0648\u0627\u0641\u0642\u0629 \u0645\u0646 \u0627\u0644\u0647\u064a\u0626\u0629 \u0627\u0644\u0639\u0627\u0645\u0629 \u0644\u0644\u0625\u062d\u0635\u0627\u0621</td>\n            </tr>\n        </thead>\n    </table>\n    \n    <table style=\"font-size:10pt; color:black; width:100%; border:1px solid black; text-align:center; direction:rtl; margin-top:35px;\">\n        <thead>\n            <tr>\n                <td style=\"border:1px solid black; vertical-align:middle !important; background-color:#d3d3d3; width:40%; font-weight:bold;\">\ufee3\ufee6</td>\n                <td style=\"border:1px solid black; vertical-align:middle !important; background-color:#d3d3d3; width:40%; font-weight:bold;\">\ufebb\ufeee\ufead\ufe93 \ufe87\ufedf\ufef0</td>\n                <td style=\"border:1px solid black; vertical-align:middle !important; background-color:#d3d3d3; width:20%; font-weight:bold;\">\ufe8d\ufef9\ufe9f\ufeae\ufe8d\ufe80</td>\n            </tr>\n        </thead>\n        \n        <tbody>\n            <tr>\n                <td style=\"border:1px solid black; vertical-align:middle !important;\">{{ doc.first_level_task }}</td>\n                <td style=\"border:1px solid black; vertical-align:middle !important;\">{{ frappe.db.get_value(\"Employee\", doc.direct_manager, \"employee_name\") or \"\"}}</td>\n                <td style=\"border:1px solid black; vertical-align:middle !important;\">{% if doc.fl_status == \"Completed\" %}  \u062a\u0645\u062a \u0627\u0644\u0645\u0648\u0627\u0641\u0642\u0629  {% else %}  \u0642\u064a\u062f \u0627\u0644\u0627\u0639\u062a\u0645\u0627\u062f  {% endif %}</td>\n            </tr>\n            \n            {% for row in doc.evacuation_second_level_tasks %}\n                <tr>\n                    <td style=\"border:1px solid black; vertical-align:middle !important;\">{{ row.task_name }}</td>\n                    <td style=\"border:1px solid black; vertical-align:middle !important;\">{{ row.full_name }}</td>\n                   <td style=\"border:1px solid black; vertical-align:middle !important;\">{% if row.status == \"Completed\" %}  \u062a\u0645\u062a \u0627\u0644\u0645\u0648\u0627\u0641\u0642\u0629  {% else %}  \u0642\u064a\u062f \u0627\u0644\u0627\u0639\u062a\u0645\u0627\u062f  {% endif %}</td>\n                </tr>\n            {% endfor %}\n            \n            \n            {% for row  in doc.evacuation_third_level_tasks %}\n                <tr>\n                    <td style=\"border:1px solid black; vertical-align:middle !important;\">{{ row.task_name }}</td>\n                    <td style=\"border:1px solid black; vertical-align:middle !important;\">{{ row.full_name }}</td>\n                   <td style=\"border:1px solid black; vertical-align:middle !important;\">{% if row.status == \"Completed\" %}  \u062a\u0645\u062a \u0627\u0644\u0645\u0648\u0627\u0641\u0642\u0629  {% else %}  \u0642\u064a\u062f \u0627\u0644\u0627\u0639\u062a\u0645\u0627\u062f  {% endif %}</td>\n                </tr>\n            {% endfor %}\n        </tbody>\n    </table>\n    \n</div>\n\n{% if print_settings.repeat_header_footer %}\n<div id=\"footer-html\">\n    {% if footer %}\n    <div class=\"letter-head-footer\"> {{ footer }} </div>\n    {% endif %}\n    <p class=\"text-center small page-number visible-pdf\">\n        {{ _(\"Page {0} of {1}\").format('<span class=\"page\"></span>', '<span class=\"topage\"></span>') }} </p>\n</div>\n{% endif %}",
